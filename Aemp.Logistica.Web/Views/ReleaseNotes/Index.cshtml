@model dynamic

@{
    ViewBag.Title = "Release Notes - Cambios y Mejoras";    
}

@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title</h1>
                @*
                <br/>
                <h2>@ViewBag.Message</h2>
                *@
            </hgroup>
            <p>
                Lista detallada de los cambios hechos en la aplicacion ordenados por fecha.
            </p>
        </div>
    </section>
}
