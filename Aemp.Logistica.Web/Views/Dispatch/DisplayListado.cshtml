@model Aemp.Logistica.Web.Models.DispatchModel
@{
    ViewBag.Title = "Transportista - Consulta de Listado";    
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <div id="detail_up" class="float-right"><img src="~/Images/deploy_chevronup.gif"/></div>
            <div id="detail_down" class="float-right"><img src="~/Images/deploy_chevrondown.gif"/></div>
            <hgroup class="title">
                <h1>@ViewBag.Title</h1>                
                <br/>
                <h2>@ViewBag.Message</h2>                
            </hgroup>
            <div id="featured_detail">
                <p>
                    <h5>Apuntes de Diseño</h5>
                    Esta pagina permite al transportista consultar los detalles de un determinado listado.<br/>
                    <h5>Observaciones</h5>
                    Esta pagina deberia contener una funcion para imprimir el listado.
                </p>
            </div>
        </div>
    </section>
}
@Html.Partial("Listado", Model)
@section scripts
{
    <script src="~/Scripts/feature-detail.js" type="text/javascript"></script>
}
