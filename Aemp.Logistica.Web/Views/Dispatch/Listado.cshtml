@model Aemp.Logistica.Web.Models.DispatchModel

<table id="header">
    <tr>
        <td>@Html.LabelFor(m => m.Transportista)</td>
        <td>@Html.DisplayFor(m => m.Transportista)</td>
    </tr>
    <tr>
        <td>@Html.LabelFor(m => m.Fecha)</td>
        <td>@Html.DisplayFor(m => m.Fecha)</td>
    </tr>
    <tr>
        <td>@Html.LabelFor(m => m.PedidoReferencia)</td>
        <td>@Html.DisplayFor(m => m.PedidoReferencia)</td>
    </tr>
    <tr>
        <td>@Html.LabelFor(m => m.Camion)</td>
        <td>@Html.DisplayFor(m => m.Camion)</td>
    </tr>
</table>
<table id="lineas">
    <tr>
        <th>Linea</th>
        <th>Tipo Producto</th>
        <th>Producto</th>
        <th>Unidad</th>
        <th>Cantidad</th>
        <th>Puesto</th>
        <th>Comerciante</th>
    </tr>
    @foreach (var linea in Model.Lineas)
    {
        <tr>
            <td>@linea.LineaId</td>
            <td>@linea.TipoProducto</td>
            <td>@linea.Producto</td>
            <td>@linea.Unidad</td>
            <td>@linea.Cantidad</td>
            <td>@linea.PuestoDescription()</td>
            <td>@linea.Comerciante</td>
        </tr>
    }
</table>

